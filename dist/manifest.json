{
  "manifest_version": 3,
  "name": "Jenkins Genie",
  "description": "Filter Jenkins builds by user, branch, status, or date.",
  "version": "1.0.0",
  "action": {
    "default_title": "Jenkins Genie",
    "default_popup": "index.html",
    "default_icon": {
      "19": "genie.png",
      "38": "genie.png"
    }
  },
  "icons": {
    "16": "genie.png",
    "48": "genie.png",
    "128": "genie.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "webRequest"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "host_permissions": [
    "https://*.runwayci.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.main.jsx.c5d1ba41.d1b8e316.js"
      ],
      "matches": [
        "https://blank.org/*",
        "https://*.runwayci.com/job/*/",
        "https://*.runwayci.com/*/job/*/"
      ],
      "run_at": "document_end",
      "css": [
        "assets/main.4b1e792d.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://*.runwayci.com/*",
        "https://*.runwayci.com/*",
        "https://blank.org/*"
      ],
      "resources": [
        "assets/client.88b335d3.js",
        "assets/main.jsx.c5d1ba41.js"
      ],
      "use_dynamic_url": true
    }
  ]
}